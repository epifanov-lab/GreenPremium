<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <rotate
        android:fromDegrees="150"
        android:pivotX="50%"
        android:pivotY="50%"
        android:toDegrees="0">
        <shape
            android:shape="line">
            <stroke
                android:width="@dimen/line_thickness_2"
                android:color="@color/green_3" />
        </shape>
    </rotate>
</item>
</layer-list>